<div align="center">
	<%=form_with url: "/static/make",method: :post,:multipart => true do %>
		<table>
			<tbody>
				<tr>
					<td>Thời gian bắt đầu</td>
					<td><%=text_field_tag :begintime,"9h00"%></td>
				</tr>
				<tr>
					<td>Thời gian kết thúc</td>
					<td><%=text_field_tag :endtime,"17h25"%></td>
				</tr>
				<tr>
					<td>Thời gian phỏng vấn mỗi ca</td>
					<td><%=number_field_tag :interviewtime,40,min: 30%></td>
				</tr>
				<tr>
					<td>Thời gian nghỉ giữa 2 ca</td>
					<td><%=number_field_tag :waittime,5,min: 0 %></td>
				</tr>
				<tr>
					<td>Thời gian nghỉ trưa</td>
					<td><%=number_field_tag :lunchtime,60,min: 0 %></td>
				</tr>
				<tr>
					<td>Danh sách sinh viên</td>
					<td><%=file_field_tag :svlist%>	</td>
				</tr>
			</tbody>
		</table>		
		<%=submit_tag %>
	<% end %>
</div>
<div id="loading" align="center"><%=image_tag 'loading.gif'%></div>
<div id="timetable" align="center">
	
</div>

